{
    "settings": {
        "number_of_shards": 1,
        "number_of_replicas": 0,
        "analysis": {
            "normalizer": {
                "my_normalizer": {
                    "type": "custom",
                    "char_filter": [],
                    "filter": [
                        "lowercase",
                        "asciifolding"
                    ]
                }
            }
        }
    },
    "mappings": {
        "_doc": {
            "_meta": {
                "class": "com.chandler.spring.elasticsearch.example.entity.User",
                "version": {
                    "min": "1.0",
                    "max": "1.3"
                },
                "description":"Elasticsearch Java Client(最新版本)"
            },
            "_field_names": {
                "enabled": false
            },
            "_source": {
                "enabled": true
            },
            "properties": {
                 "id": {
                    "type": "keyword"
                },
                "useName": {
                    "type": "keyword"
                },
                "password": {
                    "type": "keyword"
                },
                "age": {
                    "type": "short"
                },
                "brithday": {
                    "type": "date",
                    "store": true,
                    "format": "date",
                    "null_value": "NULL"
                },
                "addresses": {
                    "type": "text",
                    "store": true,
                    "fields": {
                        "company": {
                            "type": "keyword"
                        },
                        "home": {
                            "type": "keyword"
                        }
                    }
                },
                "introduction": {
                    "type": "text",
                    "similarity": "BM25",
                    "analyzer": "standard"
                },
                "memberFlag": {
                    "type": "boolean"
                },
                "tags": {
                    "type": "keyword"
                },
                "active": {
                    "type": "long",
                    "doc_values": true
                },
                "session_data": {
                    "enabled": false,
                    "dynamic": true,
                    "properties": {}
                },
                "onlineTime": {
                    "type": "date",
                    "format": "epoch_millis"
                },
                "@timestamp": {
                    "format": "yyyy-MM-dd HH:mm:ss",
                    "type": "date"
                }
            }
        }
    }
}s